{
  "name": "microsoft-computer-vision",
  "version": "0.0.1",
  "author": "Xiaoyu Zhou <xiaoyuz2011@gmail.com>",
  "main": "lib/index.js",
  "engines": {
    "node": ">=5.0 <6",
    "npm": ">=3.3 <4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/viane/microsoft-computer-vision.git"
  },
  "license": "MIT",
  "scripts": {
    "test": "mocha tests/* --compilers js:babel-register --require ./tests/helper.js --recursive",
    "start": "babel-node ./src/entry.js",
    "lint": "eslint --ext .js ./",
    "lint:fix": "npm run lint -- --fix",
    "build": "npm run lint && babel src -d lib"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ],
    "plugins": [
      "transform-flow-strip-types",
      "add-module-exports"
    ]
  },
  "devDependencies": {
    "app-module-path": "^1.0.6",
    "babel-cli": "^6.7.5",
    "babel-eslint": "^6.0.2",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-transform-flow-strip-types": "^6.7.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-register": "^6.7.2",
    "chai": "^3.5.0",
    "eslint": "^2.7.0",
    "eslint-config-airbnb": "^7.0.0",
    "eslint-plugin-babel": "^3.2.0",
    "eslint-plugin-flow-vars": "^0.3.0",
    "eslint-plugin-flowtype": "^2.2.6",
    "mocha": "^2.4.5",
    "pretty-error": "^2.0.0"
  },
  "bugs": {
    "url": "https://github.com/viane/microsoft-computer-vision/issues"
  },
  "homepage": "https://github.com/Beingbook/npm-module-template#readme",
  "directories": {
    "test": "tests"
  },
  "dependencies": {
    "app-root-path": "^2.0.1",
    "load-json-file": "^2.0.0",
    "request": "^2.79.0",
    "request-promise": "^4.1.1"
  },
  "keywords": [
    "microsoft",
    "cognitive",
    "computer",
    "vision"
  ]
}
